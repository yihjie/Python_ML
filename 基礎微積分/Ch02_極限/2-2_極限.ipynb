   "source": [
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x = [x for x in range(1, 101)]      # 相當於數列 n\n",
    "y = [1 / y for y in x ]\n",
    "\n",
    "plt.axis([0, 100, 0, 2])\n",
    "plt.plot(x, y)\n",
    "plt.xlabel(\"n\")\n",
    "plt.ylabel(\"y\")\n",
    "\n",
    "plt.grid()\n",
    "plt.show()"
   ]
